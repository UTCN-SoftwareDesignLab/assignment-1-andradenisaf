<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="205.0" prefWidth="202.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Pane fx:id="changePasswordPanel" layoutY="7.0" prefHeight="200.0" prefWidth="200.0">
         <children>
            <PasswordField fx:id="oldPasswordField" layoutX="17.0" layoutY="26.0" promptText="Old password" />
            <PasswordField fx:id="newPasswordField" layoutX="17.0" layoutY="73.0" promptText="New password" />
            <PasswordField fx:id="confirmPasswordField" layoutX="17.0" layoutY="120.0" promptText="Confirm new password" />
            <Button fx:id="changeButton" layoutX="71.0" layoutY="159.0" mnemonicParsing="false" onAction="#changePassword" text="Change" />
         </children>
      </Pane>
   </children>
</AnchorPane>
